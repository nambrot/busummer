<h1>Listing listings</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Low budget</th>
    <th>High budget</th>
    <th>Start</th>
    <th>End</th>
    <th>Location</th>
    <th>Description</th>
    <th>Seller</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
    <td><%= listing.name %></td>
    <td><%= listing.email %></td>
    <td><%= listing.low_budget %></td>
    <td><%= listing.high_budget %></td>
    <td><%= listing.start %></td>
    <td><%= listing.end %></td>
    <td><%= listing.location %></td>
    <td><%= listing.description %></td>
    <td><%= listing.seller %></td>
    <td><%= link_to 'Show', listing %></td>
    <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
    <td><%= link_to 'Destroy', listing, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Listing', new_listing_path %>
